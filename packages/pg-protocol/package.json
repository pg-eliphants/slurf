{
    "name": "pg-protocol-jkf",
    "version": "2.0.0",
    "description": "The postgres client/server binary protocol, implemented in TypeScript",
    "keywords": [],
    "bugs": {
        "url": " https: //github.com/postgres-based/node-jumbo/issues"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:postgres-based/node-jumbo.git",
        "directory": "packages/pg-protocol"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "author": "Jacob K.F. Bogers <jkfbogers@gmail.com>",
